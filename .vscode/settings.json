{
  "path-autocomplete.excludedItems": {
    "**/*.{js,jsx,ts,tsx}": {"when": "**/*.{js,jsx,ts,tsx}"},
    "**/!(*.{css,less,scss,sass,styl,stylus})": {"when": "**/*.{css,less,scss,sass,styl,stylus}", "context": "import(?!.*['\"]@\\/.*).*"},
    "**": { "when": "**/*.{js,jsx,ts,tsx}", "isDir": true }
  },
  "path-autocomplete.transformations": [{
    "type": "replace",
    "parameters": ["\\.(js|ts|jsx|tsx|vue)$", ""],
    "when": {
        "fileName": "\\.(js|ts|jsx|tsx|vue)$"
    }
  }],
  "path-autocomplete.extensionOnImport": true,
  "path-autocomplete.pathMappings": {
    "@": "${folder}/src",
  },
  "liveServer.settings.port": 5501
}
